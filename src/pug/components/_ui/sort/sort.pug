mixin sort(data)
  .swiper.sort
    .swiper-wrapper
      if data.showActive
        .swiper-slide
          +main-btn({
            tag: 'a',
            href: "#",
            cls: 'sort-btn active',
            title: 'активная'
          })
      each title in data.titles
        .swiper-slide
          +main-btn({
            tag: 'a',
            href: "#",
            cls: 'sort-btn',
            title
          })